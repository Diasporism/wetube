<div id='search' ng-app=''>
  <div ng-controller='SearchCtrl'>
    <input type='text' ng-model='data.message'>
    {{ search.results }}
  </div>
</div>


<script type='text/javascript'>

  SearchYoutube = function(criteria, $http) {
    delete $http.defaults.headers.common['X-Requested-With'];
    var response = $http.get('https://gdata.youtube.com/feeds/api/videos?q=' + criteria + '&alt=json&max-results=10');
    console.log(response);
    return response;
  }

  function SearchCtrl($scope, $http) {
    var criteria = $scope.data = {message: ''};
    $scope.search = { results: SearchYoutube(criteria, $http) }
  }

</script>